    script(src='javascripts/jquery.singlePageNav.min.js')
    // jquery.fancybox.pack
    script(src='javascripts/jquery.fancybox.pack.js')
    // Google Map API
    script(src='http://maps.google.com/maps/api/js?sensor=false')
    // Owl Carousel
    script(src='javascripts/owl.carousel.min.js')
    // jquery easing
    script(src='javascripts/jquery.easing.min.js')
    // Fullscreen slider
    script(src='javascripts/jquery.slitslider.js')
    script(src='javascripts/jquery.ba-cond.min.js')
    // onscroll animation
    script(src='javascripts/wow.min.js')
    // Custom Functions
    script(src='javascripts/main.js')
