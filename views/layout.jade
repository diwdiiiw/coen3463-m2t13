doctype html

html.no-js(lang='en')

  head
   
    meta(charset='utf-8')
    
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    title The Event Website
    
    meta(name='description', content='The Event Website')
        
    meta(name='viewport', content='width=device-width, initial-scale=1')
    //
      CSS
      ==================================================
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,700', rel='stylesheet', type='text/css')
    // Fontawesome Icon font
    link(rel='stylesheet', href='stylesheets/font-awesome.min.css')
    // bootstrap.min
    link(rel='stylesheet', href='stylesheets/jquery.fancybox.css')
    // bootstrap.min
    link(rel='stylesheet', href='stylesheets/bootstrap.min.css')
    // bootstrap.min
    link(rel='stylesheet', href='stylesheets/owl.carousel.css')
    // bootstrap.min
    link(rel='stylesheet', href='stylesheets/slit-slider.css')
    // bootstrap.min
    link(rel='stylesheet', href='stylesheets/animate.css')
    // Main Stylesheet
    link(rel='stylesheet', href='stylesheets/main.css')
    // Modernizer Script for old Browsers
    script(src='javascripts/modernizr-2.6.2.min.js')
  body

    block content

    footer#footer
      .container
        .row.text-center
          .footer-content
            .wow.animated.fadeInDown
              p Contact Us 
              p sample footer add ons only no links :>
            form.subscribe-form.wow.animated.fadeInUp(action='#', method='post')
              .input-field
                input.subscribe.form-control(type='email', placeholder='Enter Your Email...')
                button.submit-icon(type='button', onclick="location.href='/contact';")

                  i.fa.fa-paper-plane.fa-lg
            .footer-social
              ul
                li.wow.animated.zoomIn
                  a(href='#')
                    i.fa.fa-thumbs-up.fa-3x
                li.wow.animated.zoomIn(data-wow-delay='0.3s')
                  a(href='#')
                    i.fa.fa-twitter.fa-3x
                li.wow.animated.zoomIn(data-wow-delay='0.6s')
                  a(href='#')
                    i.fa.fa-skype.fa-3x
                li.wow.animated.zoomIn(data-wow-delay='0.9s')
                  a(href='#')
                    i.fa.fa-dribbble.fa-3x
                li.wow.animated.zoomIn(data-wow-delay='1.2s')
                  a(href='#')
                    i.fa.fa-youtube.fa-3x
            p
